if __name__ == '__main__':
    s = input()

    alnum=0
    alpha=0
    digit=0
    lower=0
    upper=0
    
    for i in s:
        if i.isalnum():
            alnum+=1
        if i.isalpha():
            alpha+=1
        if i.isdigit():
            digit+=1
        if i.islower():
            lower+=1
        if i.isupper():
            upper+=1
    
    if alnum ==0:
        print(False)
    else :
        print(True)

    if alpha ==0:
        print(False)
    else :
        print(True)
    
    if digit ==0:
        print(False)
    else :
        print(True)

    if lower ==0:
        print(False)
    else :
        print(True)

    if upper == 0:
        print(False)
    else :
        print(True)

    
    
